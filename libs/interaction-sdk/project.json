    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/interaction-sdk/**/*.ts",
          "libs/interaction-sdk/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
