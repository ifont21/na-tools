{
  "projectType": "library",
  "sourceRoot": "libs/interaction-sdk/src",
  "prefix": "na-transformation",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/interaction-sdk"],
      "options": {
        "jestConfig": "libs/interaction-sdk/jest.config.ts",
        "passWithNoTests": true
      }
